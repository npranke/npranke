{
    "name": "npranke",
    "version": "0.0.1",
    "private": true,
    "license": "UNLICENSED",
    "author": "npranke <nicole@npranke.io>",
    "repository": "git@github.com:npranke/npranke.git",
    "description": "bees, goats, and plums",
    "main": "app.js",
    "engines": {
        "node": "8.7.0",
        "npm": "5.4.2",
        "yarn": "1.13.0"
    },
    "dependencies": {
        "react": "^16.0.0",
        "react-dom": "^16.0.0"
    },
    "devDependencies": {
        "@babel/core": "^7.3.4",
        "@babel/preset-env": "^7.3.4",
        "@babel/preset-react": "^7.0.0",
        "babel-jest": "^24.4.0",
        "babel-loader": "^8.0.5",
        "clean-webpack-plugin": "^0.1.17",
        "css-loader": "^0.28.7",
        "enzyme": "^3.9.0",
        "enzyme-adapter-react-16": "^1.10.0",
        "enzyme-to-json": "^3.3.5",
        "eslint": "^4.9.0",
        "eslint-config-airbnb": "^16.1.0",
        "eslint-plugin-import": "^2.8.0",
        "eslint-plugin-jsx-a11y": "^6.0.2",
        "eslint-plugin-react": "^7.4.0",
        "extract-text-webpack-plugin": "^3.0.1",
        "jest": "^24.4.0",
        "jest-haste-map": "^24.4.0",
        "jest-resolve": "^24.4.0",
        "uglifyjs-webpack-plugin": "^1.0.0-beta.3",
        "webpack": "^3.8.1",
        "webpack-merge": "^4.1.0"
    },
    "scripts": {
        "build:dev": "webpack --progress --config webpack.dev.js",
        "build:prod": "webpack --progress --config webpack.prod.js",
        "heroku-postbuild": "yarn install --production=false && yarn run build:prod",
        "lint": "eslint --ext .js --ext .jsx ./static/js/",
        "lint:fix": "eslint --ext .js --ext .jsx --fix ./static/js/",
        "test": "jest --coverage",
        "test:snapup": "jest --updateSnapshot",
        "watch:build": "webpack --config webpack.dev.js --watch",
        "watch:tests": "jest --watchAll"
    },
    "jest": {
        "snapshotSerializers": [
            "enzyme-to-json/serializer"
        ]
    }
}
