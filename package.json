{
    "name": "npranke",
    "version": "0.0.1",
    "private": true,
    "license": "UNLICENSED",
    "author": "npranke <nicole@npranke.io>",
    "repository": "git@github.com:npranke/npranke.git",
    "description": "bees, goats, and plums",
    "main": "app.js",
    "engines": {
        "node": "12.4.0",
        "npm": "6.9.0",
        "yarn": "1.16.0"
    },
    "dependencies": {
        "react": "^16.8.4",
        "react-dom": "^16.8.4"
    },
    "devDependencies": {
        "@babel/core": "^7.3.4",
        "@babel/preset-env": "^7.3.4",
        "@babel/preset-react": "^7.0.0",
        "babel-jest": "^24.5.0",
        "babel-loader": "^8.0.5",
        "clean-webpack-plugin": "^2.0.0",
        "css-loader": "^2.1.1",
        "dotenv-webpack": "^1.7.0",
        "enzyme": "^3.9.0",
        "enzyme-adapter-react-16": "^1.11.0",
        "enzyme-to-json": "^3.3.5",
        "eslint": "^5.15.1",
        "eslint-config-airbnb": "^17.1.0",
        "eslint-plugin-import": "^2.16.0",
        "eslint-plugin-jsx-a11y": "^6.2.1",
        "eslint-plugin-react": "^7.12.4",
        "file-loader": "^3.0.1",
        "image-webpack-loader": "^4.6.0",
        "jest": "^24.5.0",
        "jest-haste-map": "^24.5.0",
        "jest-resolve": "^24.5.0",
        "mini-css-extract-plugin": "^0.5.0",
        "optimize-css-assets-webpack-plugin": "^5.0.1",
        "uglifyjs-webpack-plugin": "^2.1.2",
        "webpack": "^4.29.6",
        "webpack-cli": "^3.2.3",
        "webpack-merge": "^4.2.1"
    },
    "scripts": {
        "build:dev": "webpack --progress --config webpack.dev.js",
        "build:prod": "webpack --progress --config webpack.prod.js",
        "heroku-prebuild": "yarn run install:prod",
        "heroku-postbuild": "yarn run build:prod",
        "install:prod": "yarn install --production=false --frozen-lockfile",
        "lint": "eslint --ext .js --ext .jsx ./static/js/",
        "lint:fix": "eslint --ext .js --ext .jsx --fix ./static/js/",
        "test": "DOTENV_CONFIG_PATH=./.env.test jest --coverage",
        "test:snapup": "DOTENV_CONFIG_PATH=./.env.test jest --updateSnapshot",
        "watch:build": "webpack --progress --config webpack.dev.js --watch",
        "watch:tests": "jest --watchAll"
    },
    "jest": {
        "moduleNameMapper": {
            "\\.(jp(e)?g|png|gif)$": "<rootDir>/static/js/__mocks__/ImageMock.js"
        },
        "setupFiles": [
            "dotenv/config"
        ],
        "snapshotSerializers": [
            "enzyme-to-json/serializer"
        ]
    }
}
